   "source": [
    "import torch\n",
    "X = torch.normal(0,1,(1,2),requires_grad = True)\n",
    "w = torch.tensor([2,-3.4])\n",
    "w *= 2\n",
    "b = 4.2\n",
    "y = torch.matmul(X,w) + b\n",
    "y.sum().backward()\n",
    "print(X)\n",
    "print(y)\n",
    "print(X.grad)"
   ]
  }
